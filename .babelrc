{
  "presets": [
    "next/babel"
  ],
  "plugins": [
    [
      "styled-components",
      {
        "ssr": true,
        "displayName": true,
        "preprocess": false,
      }
    ],
    ["module-resolver", {
      "root": ["./"],
      "alias": {
        "test": "./test",
        "underscore": "lodash"
      }
    }],
    "inline-react-svg",
    "@babel/plugin-proposal-class-properties"
  ]
}
